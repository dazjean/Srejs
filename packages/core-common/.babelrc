{
    "presets":[[
        "@babel/env",
        {
          "targets": {
            "browsers": ["last 2 versions", "ie >= 7"]
          }
        }
      ]],
      "plugins": [
        [
          "@babel/plugin-transform-runtime",
          { "helpers": false, "regenerator": true }
        ],
        "@babel/plugin-transform-modules-commonjs"
      ]
}
